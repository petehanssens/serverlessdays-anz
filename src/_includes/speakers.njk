---
layout: base.njk
---
<div class="ph3 ph0-ns pt4 pt6-ns w-100 center">
    <div class="w-100 w-80-m w-70-l center tc tl-ns">
        <h1 class="f1">Speakers</h1>
            {% for key,details in speakers %}
            <span class="w5 pa2 pa3-m pa4-l v-top center tc dib">
                <a class="no-underline center primaryColor" href="/speakers/{{ key }}">
                    <img class="ba br-100 b--transparent w5" src="{{ details.img }}" alt="{{ details.name }}" />
                    <div class="f3 fw6 pv2 tc">{{ details.name }}</div>
                    <div class="f5">{{details.title}}</div>
                </a>
            </span>
            {% endfor %}
    </div>
    <div class="cf">
    </div>
</div>